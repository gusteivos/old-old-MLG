# includeh "entry.mlh"


thiscope signed integer p_a0 = 50I;

thiscope signed integer p_b0 = 19I;


thiscope func sum(signed integer a, signed integer b) : signed integer
{

    return a + b;

}


# if not defined USE_ENTRY

    sum(p_a0, p_b0);

# else

    func entry() : none
    {

        sum(p_a0, p_b0);

    }

# end
